<h1>Add a new item</h1>

<form action="/make_a_new_item" method="post">
<div>
    <label for="link_url">Link URL</label>
    <input type="text" name="link_url" id="link_url">
  </div>

  <div>
    <label for="link_description">Link Description</label>
    <input type="text" name="link_description" id="link_description">
  </div>

  <div>
    <label for="thumbnail_url">Thumbnail URL</label>
    <input type="text" name="thumbnail_url" id="thumbnail_url">
  </div>

  <button type="submit">Create item</button>
</form>
